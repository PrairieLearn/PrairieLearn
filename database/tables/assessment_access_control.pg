indexes
    assessment_access_control_pkey: PRIMARY KEY (id) USING btree (id)
    aac_ci_assessment_number_target_type_unique: UNIQUE (course_instance_id, assessment_id, number, target_type) USING btree (course_instance_id, assessment_id, number, target_type)
    assessment_access_control_id_target_type_idx: UNIQUE USING btree (id, target_type)

check constraints
    assessment_access_control_target_type_check: CHECK (target_type = ANY (ARRAY['none'::text, 'enrollment'::text, 'student_label'::text]))
    check_first_rule_is_none: CHECK ((number = 0) = (target_type = 'none'::text))

foreign-key constraints
    assessment_access_control_assessment_id_fkey: FOREIGN KEY (assessment_id) REFERENCES assessments(id) ON UPDATE CASCADE ON DELETE CASCADE
    assessment_access_control_course_instance_id_fkey: FOREIGN KEY (course_instance_id) REFERENCES course_instances(id) ON UPDATE CASCADE ON DELETE CASCADE

