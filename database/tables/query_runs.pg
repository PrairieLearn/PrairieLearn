columns
    id: bigint not null default nextval('news_item_notifications_id_seq'::regclass)
    date: timestamp with time zone not null default CURRENT_TIMESTAMP
    error: text
    name: text not null
    params: jsonb
    result: jsonb
    sql: text not null
    user_id: bigint not null

indexes
    query_runs_pkey: PRIMARY KEY (id) USING btree (id)

foreign-key constraints
    query_runs_user_id_fkey: FOREIGN KEY (user_id) REFERENCES users(user_id) ON UPDATE CASCADE ON DELETE SET NULL
