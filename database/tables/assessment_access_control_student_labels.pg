columns
    assessment_access_control_id: bigint not null
    id: bigint not null default nextval('assessment_access_control_student_labels_id_seq'::regclass)
    student_label_id: bigint not null
    target_type: text not null default 'student_label'::text

indexes
    assessment_access_control_student_labels_pkey: PRIMARY KEY (id) USING btree (id)
    assessment_access_control_stu_assessment_access_control_id__key: UNIQUE (assessment_access_control_id, student_label_id) USING btree (assessment_access_control_id, student_label_id)
    assessment_access_control_student_labels_aac_id_idx: USING btree (assessment_access_control_id)
    assessment_access_control_student_labels_sl_id_idx: USING btree (student_label_id)

check constraints
    assessment_access_control_student_labels_target_type_check: CHECK (target_type = 'student_label'::text)

foreign-key constraints
    assessment_access_control_stu_assessment_access_control_id_fkey: FOREIGN KEY (assessment_access_control_id, target_type) REFERENCES assessment_access_control(id, target_type) ON UPDATE CASCADE ON DELETE CASCADE
    assessment_access_control_student_labels_student_label_id_fkey: FOREIGN KEY (student_label_id) REFERENCES student_labels(id) ON UPDATE CASCADE ON DELETE CASCADE
