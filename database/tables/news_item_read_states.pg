columns
    id: bigint not null default nextval('news_item_read_states_id_seq'::regclass)
    last_read_at: timestamp with time zone not null default now()
    user_id: bigint not null

indexes
    news_item_read_states_pkey: PRIMARY KEY (id) USING btree (id)
    news_item_read_states_user_id_key: UNIQUE (user_id) USING btree (user_id)

foreign-key constraints
    news_item_read_states_user_id_fkey: FOREIGN KEY (user_id) REFERENCES users(id) ON UPDATE CASCADE ON DELETE CASCADE
