columns
    fetched_at: timestamp with time zone not null default now()
    guid: text not null
    id: bigint not null default nextval('cached_news_items_id_seq'::regclass)
    link: text not null
    pub_date: timestamp with time zone not null
    title: text not null

indexes
    cached_news_items_pkey: PRIMARY KEY (id) USING btree (id)
    cached_news_items_guid_key: UNIQUE (guid) USING btree (guid)
    cached_news_items_pub_date_idx: USING btree (pub_date DESC)
