columns
    assessment_instance_id: bigint not null
    authn_user_id: bigint
    crossed_at: timestamp with time zone not null default now()
    id: bigint not null default nextval('assessment_instance_crossed_lockpoints_id_seq'::regclass)
    zone_id: bigint not null

indexes
    assessment_instance_crossed_lockpoints_pkey: PRIMARY KEY (id) USING btree (id)
    aicl_assessment_instance_id_zone_id_key: UNIQUE (assessment_instance_id, zone_id) USING btree (assessment_instance_id, zone_id)

foreign-key constraints
    aicl_assessment_instance_id_fkey: FOREIGN KEY (assessment_instance_id) REFERENCES assessment_instances(id) ON UPDATE CASCADE ON DELETE CASCADE
    assessment_instance_crossed_lockpoints_authn_user_id_fkey: FOREIGN KEY (authn_user_id) REFERENCES users(id) ON UPDATE CASCADE ON DELETE SET NULL
    assessment_instance_crossed_lockpoints_zone_id_fkey: FOREIGN KEY (zone_id) REFERENCES zones(id) ON UPDATE CASCADE ON DELETE CASCADE
