columns
    id: bigint not null default nextval('user_news_read_timestamps_id_seq'::regclass)
    last_read_at: timestamp with time zone not null default now()
    user_id: bigint not null

indexes
    user_news_read_timestamps_pkey: PRIMARY KEY (id) USING btree (id)
    user_news_read_timestamps_user_id_key: UNIQUE (user_id) USING btree (user_id)

foreign-key constraints
    user_news_read_timestamps_user_id_fkey: FOREIGN KEY (user_id) REFERENCES users(id) ON UPDATE CASCADE ON DELETE CASCADE
