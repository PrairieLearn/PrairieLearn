columns
    enrollment_id: bigint not null
    id: bigint not null default nextval('student_group_enrollments_id_seq'::regclass)
    student_group_id: bigint not null

indexes
    student_group_enrollments_pkey: PRIMARY KEY (id) USING btree (id)
    student_group_enrollments_enrollment_id_student_group_id_key: UNIQUE (enrollment_id, student_group_id) USING btree (enrollment_id, student_group_id)
    student_group_enrollments_student_group_id_key: USING btree (student_group_id)

foreign-key constraints
    student_group_enrollments_enrollment_id_fkey: FOREIGN KEY (enrollment_id) REFERENCES enrollments(id) ON UPDATE CASCADE ON DELETE CASCADE
    student_group_enrollments_student_group_id_fkey: FOREIGN KEY (student_group_id) REFERENCES student_groups(id) ON UPDATE CASCADE ON DELETE CASCADE
