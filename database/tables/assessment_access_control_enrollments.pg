columns
    assessment_access_control_id: bigint not null
    enrollment_id: bigint not null
    id: bigint not null default nextval('assessment_access_control_enrollments_id_seq'::regclass)
    target_type: text not null default 'enrollment'::text

indexes
    assessment_access_control_enrollments_pkey: PRIMARY KEY (id) USING btree (id)
    assessment_access_control_enr_assessment_access_control_id__key: UNIQUE (assessment_access_control_id, enrollment_id) USING btree (assessment_access_control_id, enrollment_id)
    assessment_access_control_enrollments_aac_id_idx: USING btree (assessment_access_control_id)
    assessment_access_control_enrollments_enrollment_id_idx: USING btree (enrollment_id)

check constraints
    assessment_access_control_enrollments_target_type_check: CHECK (target_type = 'enrollment'::text)

foreign-key constraints
    assessment_access_control_enr_assessment_access_control_id_fkey: FOREIGN KEY (assessment_access_control_id, target_type) REFERENCES assessment_access_control(id, target_type) ON UPDATE CASCADE ON DELETE CASCADE
    assessment_access_control_enrollments_enrollment_id_fkey: FOREIGN KEY (enrollment_id) REFERENCES enrollments(id) ON UPDATE CASCADE ON DELETE CASCADE
