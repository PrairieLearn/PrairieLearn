<% if (authz_data.has_course_permission_view && question.course_id === course.id) { %>
<%- include('navTab', {
    activeSubPage:  ['file_view', 'file_edit'],
    urlSuffix:      '/question/'+question.id+'/file_view',
    iconClasses:    'fa fa-edit',
    tabLabel:       'Files'}) %>
<% } %>
<%- include('navTab', {
    activeSubPage:  'preview',
    urlSuffix:      '/question/'+question.id+'/preview',
    iconClasses:    'fas fa-tv',
    tabLabel:       'Preview'}) %>
<% if (question.course_id === course.id) { %>
<%- include('navTab', {
    activeSubPage:  'settings',
    urlSuffix:      '/question/'+question.id+'/settings',
    iconClasses:    'fas fa-cog',
    tabLabel:       'Settings'}) %>
<%- include('navTab', {
    activeSubPage:  'statistics',
    urlSuffix:      '/question/'+question.id+'/statistics',
    iconClasses:    'fas fa-chart-bar',
    tabLabel:       'Statistics'}) %>
<%- include('navTab', {
    activeSubPage:  'issues',
    urlSuffix:      '/course_admin/issues?q=is%3Aopen+qid%3A'+encodeURIComponent(question.qid),
    iconClasses:    'fas fa-bug',
    tabLabel:       'Issues',
    htmlSuffix:     include("../issueBadge", {count: open_issue_count, suppressLink: true, className: 'ml-2'})}) %>
<% } %>
