<div class="modal fade" id="submissionFeedbackModal-<%= submission.id %>" tabindex="-1" role="dialog" aria-labelledby="submissionFeedbackModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header bg-danger text-white">
        <h5 class="modal-title" id="submissionFeedbackModalLabel">Grader feedback (Submitted answer <%= submissionIdx %>)</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%- submission.manual_feedback_html %>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
