[sqlfluff]
templater = placeholder
dialect = postgres
large_file_skip_byte_limit = 0
sql_file_exts = .sql
exclude_rules = layout.indent, 
    ambiguous.join,
    capitalisation.identifiers,
    capitalisation.functions,
    layout.select_targets,
    layout.long_lines,
    layout.spacing,
    # capitalisation.keywords,
    layout.cte_newline, 
    structure.join_condition_order, 
    aliasing.unique.column, 
    structure.join_condition_order, 
    references.qualification, 
    references.from, 
    references.consistent, 
    references.keywords, 
    structure.column_order, 
    ambiguous.order_by, 
    layout.functions, 
    aliasing.column, 
    aliasing.expression, 
    structure.using, 
    aliasing.table, 
    capitalisation.literals, 
    structure.unused_cte, 
    structure.subquery,
    capitalisation.types, 
    ambiguous.column_count, 
    aliasing.unused, 
    aliasing.self_alias.column, 
    references.quoting, 
    aliasing.unique.table, 
    structure.unused_join, 
    structure.simple_case, 
    convention.count_rows, 
    structure.else_null, 
    convention.casting_style, 
    layout.start_of_file, 
    convention.left_join, 
    structure.distinct, 
    ambiguous.column_references

[sqlfluff:templater:placeholder]
param_style = dollar
start = 'start'
end = 'end'
offset = 'offset'
embedding = 'embedding'
enabled_authn_provider_ids = []
limit = 1000

[sqlfluff:rules:capitalisation.keywords]
capitalisation_policy = upper